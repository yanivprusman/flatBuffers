{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "declaration": true,
      "moduleResolution": "node"
    },
    "include": ["ts/**/*.ts"],
    "exclude": ["node_modules"]
  }